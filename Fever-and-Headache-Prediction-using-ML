        "from sklearn.model_selection import cross_val_score\n",
        "\n",
        "# Random Forest cross-validation\n",
        "rf_cv_scores = cross_val_score(rf_model, X_scaled, y, cv=5)\n",
        "print(\"Random Forest CV scores:\", rf_cv_scores)\n",
        "print(\"Average CV Accuracy:\", rf_cv_scores.mean())\n",
        "\n",
        "# KNN cross-validation\n",
        "knn_cv_scores = cross_val_score(knn_model, X_scaled, y, cv=5)\n",
        "print(\"KNN CV scores:\", knn_cv_scores)\n",
        "print(\"Average CV Accuracy:\", knn_cv_scores.mean())\n",
        "\n",
