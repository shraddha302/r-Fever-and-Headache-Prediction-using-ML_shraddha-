      "cell_type": "code",
      "source": [
        "\n",
        "import pandas as pd\n",
        "\n",
        "# Load the dataset\n",
        "data = pd.read_csv(\"/content/enhanced_fever_medicine_recommendation.csv\")\n",
        "\n",
        "# Check the first few rows\n",
        "print(data.head())\n",
        "\n",
        "from sklearn.preprocessing import LabelEncoder\n",
        "\n",
        "# Make a copy to avoid overwriting the original data\n",
        "data_encoded = data.copy()\n",
        "\n",
        "# Select categorical columns (object type)\n",
        "categorical_cols = data_encoded.select_dtypes(include=['object']).columns\n",
        "\n",
        "# Apply Label Encoding only to categorical columns\n",
        "encoder = LabelEncoder()\n",
        "for col in categorical_cols:\n",
        "    data_encoded[col] = encoder.fit_transform(data_encoded[col])\n",
        "\n",
        "\n"
      ],
