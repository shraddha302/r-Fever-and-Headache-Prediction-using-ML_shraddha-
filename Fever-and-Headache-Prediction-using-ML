        "# Common patients: e.g., no chronic conditions, age 30-60\n",
        "common_patients = X_test[(X_test['Chronic_Conditions'] == 0) & (X_test['Age'] >= 30) & (X_test['Age'] <= 60)]\n",
        "\n",
        "# Patients with multiple conditions\n",
        "multiple_conditions_patients = X_test[X_test['Chronic_Conditions'] >= 2]\n",
        "\n",
        "# Rare or edge-case patients (e.g., extreme age or unusual symptom combinations)\n",
        "rare_patients = X_test[(X_test['Age'] < 20) | (X_test['Age'] > 80)]\n"
      ],
